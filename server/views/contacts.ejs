<%- include ('partials/header.ejs'); %> 

<%- include ('partials/main_nav.ejs'); %> 

<main class="container">
    <div class="row">
        <div class="col-md-offset-3 col-md-6">
            <h1><%= title %></h1>
            <table class="table table-bordered-table-striped-hover">
                <tr>
                    <th class="text-center">First Name</th>
                    <th class="text-center">Second Name</th>
                    <th class="text-center">Email</th>
                    <th class="text-center">Message</th>
                </tr>

                <% for(let count = 0; count < ContactList.length; count++){ %>
                    <tr>
                        <td class="text-center"><%= ContactList[count].fname %> </td>
                        <td class="text-center"><%= ContactList[count].lname %> </td>
                        <td class="text-center"><%= ContactList[count].email %> </td>
                        <td class="text-center"><%= ContactList[count].message %> </td>
                    </tr>
                <% } %>
            </table>
        </div>
    </div>
</main>


<%- include ('partials/bottom_nav'); %> 

<%- include ('partials/footer.ejs'); %> 